# Pterodactyl Panel Configuration
# Copy this file to .env and update with your values

# Your Pterodactyl panel URL (without trailing slash)
PTERODACTYL_URL=http://your-panel.com

# Application API Key (for server management, starts with ptla_)
APPLICATION_API_KEY=ptla_your_application_api_key_here

# Client API Key (for file operations, starts with ptlc_)
# This can be left empty and configured through the web interface
CLIENT_API_KEY=ptlc_your_client_api_key_here

# Server port (optional, defaults to 3000)
PORT=3000
